<?xml version="1.0" encoding="UTF-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

  <item android:state_pressed="true" android:state_focused="true">
    <shape>
      <gradient android:startColor="#EBEBEB" android:centerColor="#EBEBEB" android:endColor="#EBEBEB" android:angle="270" />
      <stroke android:width="2.3dp" android:color="#F8F8F8" />
      <corners android:radius="10dp" />
    </shape>
  </item>

  <item android:state_pressed="true" android:state_focused="false">
    <shape>
      <gradient android:startColor="#EBEBEB" android:centerColor="#EBEBEB" android:endColor="#EBEBEB" android:angle="270" />
      <stroke android:width="2.3dp" android:color="#F8F8F8" />
      <corners android:radius="10dp" />
    </shape>
  </item>

  <item android:state_pressed="false" android:state_focused="true">
    <shape>
      <gradient android:startColor="#F8F8F8" android:centerColor="#F8F8F8" android:endColor="#F8F8F8" android:angle="270" />
      <stroke android:width="0.7dp" android:color="#000000" />
      <corners android:radius="10dp" />
    </shape>
  </item>

  <item android:state_pressed="false" android:state_focused="false">
    <shape>
      <gradient android:startColor="#F8F8F8" android:centerColor="#F8F8F8" android:endColor="#F8F8F8" android:angle="270" />
      <stroke android:width="0.7dp" android:color="#000000" />
      <corners android:radius="10dp" />
    </shape>
  </item>

  <item android:state_enabled="true">
    <shape>
      <padding android:left="5dp" android:top="5dp" android:right="5dp" android:bottom="5dp" />
    </shape>
  </item>

</selector>